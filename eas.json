{
  "cli": {
    "version": ">= 14.1.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096"
      },
      "node": "22.12.0"
    },
    "production": {
      "android": {
        "buildType": "aab"
      },
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096"
      },
      "node": "22.12.0"
    }
  },
  "submit": {
    "production": {}
  }
}
